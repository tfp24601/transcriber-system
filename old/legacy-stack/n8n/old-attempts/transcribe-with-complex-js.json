            "={{ 'done' }}",
            "={{ $json.transcript }}",
            "={{ $json.vttPath }}",
            "={{ $json.srtPath }}",
            "={{ $json.duration }}",
            "={{ $json.recordingId }}"
          ]
        }
      },
      "id": "update-complex-status",
      "name": "Update Complex Status",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2,
      "position": [1560, 300],
      "credentials": {
        "postgres": {
          "id": "postgres_main",
          "name": "PostgreSQL Main DB"
        }
      }
    },
    {
