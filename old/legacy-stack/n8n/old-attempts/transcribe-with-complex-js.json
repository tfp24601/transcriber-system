              {
                "name": "temperature",
                "value": "0"
              }
            ]
          }
        }
      },
      "id": "complex-transcribe",
      "name": "Complex Transcribe",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [900, 300]
    },
    {
      "parameters": {
        "jsCode": "const response = $input.first().json;\nconst recordingId = $node[\"Parse Complex Request\"].json[\"recordingId\"];\n\n// Process transcript data\nconst segments = response.segments || [];\nconst duration = response.duration || 0;\n\n// Generate VTT format\nlet vttContent = 'WEBVTT\\n\\n';\nsegments.forEach((segment, index) => {\n  const startTime = formatTime(segment.start || 0);\n  const endTime = formatTime(segment.end || segment.start + 1);\n  vttContent += `${index + 1}\\n${startTime} --> ${endTime}\\n${segment.text?.trim() || ''}\\n\\n`;\n});\n\n// Generate SRT format\nlet srtContent = '';\nsegments.forEach((segment, index) => {\n  const startTime = formatSRTTime(segment.start || 0);\n  const endTime = formatSRTTime(segment.end || segment.start + 1);\n  srtContent += `${index + 1}\\n${startTime} --> ${endTime}\\n${segment.text?.trim() || ''}\\n\\n`;\n});\n\n// Helper functions\nfunction formatTime(seconds) {\n  const hours = Math.floor(seconds / 3600);\n  const minutes = Math.floor((seconds % 3600) / 60);\n  const secs = Math.floor(seconds % 60);\n  const ms = Math.floor((seconds % 1) * 1000);\n  return `${hours.toString().padStart(2, '0')}:${minutes.toString().padStart(2, '0')}:${secs.toString().padStart(2, '0')}.${ms.toString().padStart(3, '0')}`;\n}\n\nfunction formatSRTTime(seconds) {\n  const hours = Math.floor(seconds / 3600);\n  const minutes = Math.floor((seconds % 3600) / 60);\n  const secs = Math.floor(seconds % 60);\n  const ms = Math.floor((seconds % 1) * 1000);\n  return `${hours.toString().padStart(2, '0')}:${minutes.toString().padStart(2, '0')}:${secs.toString().padStart(2, '0')},${ms.toString().padStart(3, '0')}`;\n}\n\n// Return processed data\nreturn {\n  recordingId,\n  transcript: response.text || '',\n  vttContent,\n  srtContent,\n  duration,\n  segments: segments.length,\n  textPath: `/tmp/transcript-${recordingId}.txt`,\n  vttPath: `/tmp/transcript-${recordingId}.vtt`,\n  srtPath: `/tmp/transcript-${recordingId}.srt`\n};"
      },
      "id": "process-transcript",
      "name": "Process Transcript",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1120, 300]
    },
    {
      "parameters": {
